#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/cangli/Downloads/个人博客网站开发 (1)/node_modules/.pnpm/baseline-browser-mapping@2.9.11/node_modules/baseline-browser-mapping/dist/node_modules:/home/cangli/Downloads/个人博客网站开发 (1)/node_modules/.pnpm/baseline-browser-mapping@2.9.11/node_modules/baseline-browser-mapping/node_modules:/home/cangli/Downloads/个人博客网站开发 (1)/node_modules/.pnpm/baseline-browser-mapping@2.9.11/node_modules:/home/cangli/Downloads/个人博客网站开发 (1)/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/cangli/Downloads/个人博客网站开发 (1)/node_modules/.pnpm/baseline-browser-mapping@2.9.11/node_modules/baseline-browser-mapping/dist/node_modules:/home/cangli/Downloads/个人博客网站开发 (1)/node_modules/.pnpm/baseline-browser-mapping@2.9.11/node_modules/baseline-browser-mapping/node_modules:/home/cangli/Downloads/个人博客网站开发 (1)/node_modules/.pnpm/baseline-browser-mapping@2.9.11/node_modules:/home/cangli/Downloads/个人博客网站开发 (1)/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/cli.js" "$@"
else
  exec node  "$basedir/../../dist/cli.js" "$@"
fi
